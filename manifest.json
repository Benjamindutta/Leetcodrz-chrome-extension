{
    "manifest_version": 3,
    "name": "leetcodesubmitter",
    "version": "1.0",
    "description": "an extention for scraping leetcode problem data",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "cookies"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}